platform :ios, '26.0'

target 'SSMUtility' do
  use_frameworks!

  # Pods for SSMUtility
  pod 'OpenCV', '~>4.1.0'
  
  # Sentry for error tracking and performance monitoring
  pod 'Sentry', :git => 'https://github.com/getsentry/sentry-cocoa.git', :tag => '9.1.0'

  # Google AdMob (SDK v12+)
  pod 'Google-Mobile-Ads-SDK'

end
